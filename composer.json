{
    "name": "therour/laravel-actionable",
    "description": "Single Class Action Handler for Laravel Framework",
    "keywords": ["laravel", "controller", "action"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nizom Sidiq",
            "email": "nizomsidiq@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "illuminate/routing": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/support": "~5.4",
        "illuminate/container": "~5.4",
        "illuminate/contracts": "~5.4",
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.5",
        "orchestra/testbench-browser-kit": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Therour\\Actionable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Therour\\Actionable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Therour\\Actionable\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
